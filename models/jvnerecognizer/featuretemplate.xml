<VnNameRecognitionFeaSel version="3.0"
  xmlns="http://www.dmg.org/PMML-3-0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema_instance"  >
	<CPType value = "Conjunction">
		<ctxpre value = "0:1:word_conj:0"/>		
	 	<ctxpre value = "0:1:word_conj:-1"/>		
		<ctxpre value = "0:1:word_conj:1"/>		
		<ctxpre value = "0:1:word_conj:2"/>		
		<ctxpre value = "0:1:word_conj:-2"/>
		<ctxpre value = "0:1:word_conj:-3"/>			
		<ctxpre value = "0:1:word_conj:3"/>
		<ctxpre value = "0:1:word_conj:-2:-1"/>		
		<ctxpre value = "0:1:word_conj:-3:-2:-1"/>		
		<ctxpre value = "0:1:word_conj:-2:-1:0"/>		
		<ctxpre value = "0:1:word_conj:0:1"/>		
		<ctxpre value = "0:1:word_conj:1:2"/>				
	</CPType>			
	<CPType value="Lexicon">		
		<ctxpre value= "0:2:in_vnProvinces:0"/>
		<ctxpre value= "0:2:in_vnProvinces:0:1"/>
		<ctxpre value= "0:2:in_vnProvinces:-1:0"/>
		<ctxpre value= "0:2:in_vnPersonalPositions:-1"/>		
		<ctxpre value= "0:2:in_vnPersonalPositions:-2:-1:0"/>		
		<ctxpre value= "0:2:in_vnPersonalPositions:-2:-1"/>
		<ctxpre value="0:2:in_vnPersonalPronouns:0"/>
		<ctxpre value= "0:2:has_vnFirstNames:0" />
		<ctxpre value= "0:2:has_vnMiddleNames:0" />		
		<ctxpre value= "0:2:has_vnLastNames:0" />
		<ctxpre value= "0:2:in_vnNums:0" />
		<ctxpre value="0:2:in_vnTimeMarkers:0"/>
		<ctxpre value="0:2:in_vnTimeMarkers:-1"/>
		<ctxpre value="0:2:in_vnTimeMarkers:1"/>
		<ctxpre value="0:2:in_countries:0"/>
		<ctxpre value="0:2:in_countries:0:1"/>
		<ctxpre value="0:2:in_countries:-1:0"/>
		<ctxpre value="0:2:and(1_in_vnPersonalPronouns,1_has_vnFirstNames):-1:0"/>
	</CPType>	
	
	<CPType value = "WordFeature">
		<ctxpre value = "0:1:initial_cap:0"/>		
		<ctxpre value = "0:1:initial_cap:-1"/>		
		<ctxpre value = "0:1:initial_cap:-2"/>		
		<ctxpre value = "0:1:all_cap:0"/>
        <ctxpre value = "0:1:mark:0"/>
		<ctxpre value = "0:1:first_obsrv:0"/>
		<ctxpre value = "0:1:contain_percent_sign:0"/>
		<ctxpre value = "0:1:contain_slash_sign:0"/>
		<ctxpre value = "0:1:contain_slash_sign:1"/>
		<ctxpre value = "0:1:contain_comma_sign:0"/>
		<ctxpre value = "0:1:all_cap_and_digit:0"/>
		<ctxpre value = "0:1:all_syll_init_caped:0"/>
	</CPType>
	
	<CPType value = "Regex">		
		<ctxpre value = "0:1:number:0" />
		<ctxpre value = "0:1:number:-1" />
		<ctxpre value = "0:1:number:1" />
		<ctxpre value = "0:1:long_date:0" />
		<ctxpre value = "0:1:long_date:1" />
		<ctxpre value = "0:1:long_date:-1" />
		<ctxpre value = "0:1:short_date:0" />
		<ctxpre value = "0:1:short_date:-1" />
		<ctxpre value = "0:1:short_date:1" />
		<ctxpre value = "0:1:percentage:0" />
		<ctxpre value = "0:1:percentage:-1" />
		<ctxpre value = "0:1:percentage:1" />
	</CPType>

</VnNameRecognitionFeaSel>
